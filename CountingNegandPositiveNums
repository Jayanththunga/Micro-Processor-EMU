DATA SEGMENT
NUM DB -3,1,-5,6,-7,9,7
LEN DW $-NUM
p_cnt db 0h
n_cnt db 0h
DATA ENDS
CODE SEGMENT
ASSUME CS:CODE DS:DATA
START:
MOV AX,DATA
MOV DS,AX
MOV CX,LEN
LEA SI,NUM
LOOPER:
MOV AL,[SI]
CMP AL,0
JG POS
JL NEGA
POS:
INC BH
JMP REST
NEGA:
INC DH
JMP REST
REST:
INC SI
DEC CX
CMP CX,0
JG LOOPER
MOV p_cnt,BH
MOV n_cnt,DH
MOV AH,4CH
INT 21H
CODE ENDS
END START